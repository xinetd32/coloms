<% jsonOptions = @jsonOptions || {} %>
{
    success: true,
  <% if @result.class == ActiveRecord::Relation %>
    total:<%= @result.except(:limit, :offset).count %>,
  <% end %>
  
  rows:[
  
  <% respond={} %>
  <% @result.each do |row| %>
  <%    respond[:id]=row.id %>    
  <%    respond[:created_at]=row.created_at %>
  <%    respond[:updated_at]=row.updated_at %>  
  <%    respond[:description]=row.description %>
  <%    respond[:product_type]=row.product_type.name %> 
  <%    respond[:vendor_name]=row.vendor.name %>    
  <%    respond[:model_name]=row.model.name %>
  <%    respond[:model_power]=row.model.power %> 
  <%    respond[:order_id]= row.order.id %>
  <%    respond[:condition]= row.condition %> 
  <%    respond[:guaranty]= row.guaranty %>
  <%    respond[:guaranty_service]= row.guaranty_service %>
  <%    respond[:status]= row.status %> 
  <%    respond[:location_id]= row.location_id %>
  <%    respond[:price]= row.price %>  
  <%    respond[:old_id]= row.old_id %>    
  <%    respond[:_location]= row.location ? row.location.name : '' %>    
    
  <%= respond.to_json(jsonOptions).to_s.html_safe %>,
  <% end %>
  
  ]
}

